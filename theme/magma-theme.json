				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#ee7b06"
			}
		},
		{
			"name": "Fountain Blue",
			"scope": ["constant", "keyword.other.unit"],
			"settings": {
				"foreground": "#ffa904"
			}
		},
		{
			"name": "Shuttle Gray",
			"scope": ["comment"],
			"settings": {
				"foreground": "#5c6370"
			}
		},
		{
			"name": "Olivine",
			"scope": ["string", "markup.quote", "markup.bold", "markup.italic"],
			"settings": {
				"foreground": "#8454A0"
			}
		},
		{
			"name": "Whiskey",
			"scope": [
				"entity.name.class",
				"entity.name.function",
				"entity.name.type",
				"entity.other.attribute-name",
				"entity.other.inherited-class",
				"markup.heading.setext",
